for arg in 192.168.8.88 192.168.9.87 192.168.9.84
do
  echo === $arg ==
  ( echo 'set_debug_level Platform 4'; \
    echo 'set_debug_level Memory 2';   \
    echo 'set_debug_level TCP 4';      \
    echo 'set_debug_level Client 4';   \
    echo 'set_debug_level Protocol 4'; \
    echo 'set_debug_level Queue 2';    \
    echo 'get_debug_level'; sleep 2 )  \
    | telnet $arg 40000
done

